    <lightson_time datatype="time24" range_basic="" range_advanced="" units="" appear_basic="true" appear_advanced="true" entry="manual" description="Time the lights are turned on everyday." required="true" default="$LAST" last="08:00" />
    <lightsoff_time datatype="time24" range_basic="" range_advanced="" units="" appear_basic="true, readonly" appear_advanced="true" entry="manual" description="Time the lights are turned off everyday." required="true" default="$LAST" last="16:00" />
