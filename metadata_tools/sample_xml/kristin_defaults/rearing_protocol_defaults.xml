  <temperature_setpoint datatype="float" range_basic="[-Inf, Inf]" range_advanced="[-Inf, Inf]" units="degrees Celsius" appear_basic="true" appear_advanced="true" entry="manual" description="Set point of rearing incubator in deg. C " required="true" default="$LAST" last="25"></temperature_setpoint>
  <humidity datatype="float" range_basic="[0,100]" range_advanced="[0,100]" units="percent" appear_basic="true" appear_advanced="true" entry="manual" description="Set point of rearing incubator in %RH." required="true" default="$LAST" last="60"></humidity>
